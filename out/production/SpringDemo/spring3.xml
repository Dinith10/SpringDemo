<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd" >

<beans>
    
    
    
        <bean id = "circle" class = "mainPackage.Circle">

            <property name="points">

                <list>

                    <ref bean = "zeroPoint"/>
                    <ref bean = "point1"/>
                    <ref bean = "point2"/>

                </list>


            </property>

            <property name="intvalues">

                <list>
                    <value>12</value>
                    <value>21</value>


                </list>

            </property>




            
        </bean>

    <bean id = "zeroPoint" class = "mainPackage.Point">

        <property name="x" value="0"/>
        <property name="y" value="0"/>


    </bean>

    <bean id="point1" class="mainPackage.Point">

        <property name="x" value="20"/>
        <property name="y" value="0"/>

    </bean>


    <bean id="point2" class = "mainPackage.Point">

        <property name="x" value="12"/>
        <property name="y" value="21"/>

    </bean>

</beans>